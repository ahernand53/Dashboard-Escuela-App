@*
    @model List<ObjetoEscuelaBase>
*@


<div class="card">
    <table class="table table-hover">
        <thead class="bg-dark text-light">
            <tr>
                <th scope="col">Id</th>
                <th scope="col">Nombre</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var obj in Model)
            {
                <tr>
                    <td>@obj.Id</td>
                    <td>@obj.Nombre</td>
                </tr>
            }
        </tbody>
    </table>
</div>